{
  "name": "react-easy-spinners",
  "version": "1.0.0",
  "description": "⚛️ A CSS-only spinners in React components",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint **/*.js",
    "lint:fix": "prettier-eslint $PWD/**/*.js --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-ARTS/react-easy-spinners.git"
  },
  "keywords": [
    "css-spinners",
    "react-spinners"
  ],
  "author": "Hanan Khan Mufti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h-ARTS/react-easy-spinners/issues"
  },
  "homepage": "https://github.com/h-ARTS/react-easy-spinners#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "rollup": "^2.15.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^6.1.0"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "engines": {
    "node": ">=12"
  }
}
